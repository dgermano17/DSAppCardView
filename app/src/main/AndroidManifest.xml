<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.etecia.appcardview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity_Tempo" android:theme="@style/TemaSemBarra"></activity>
        <activity android:name=".Activity_Configuracoes" />
        <activity android:name=".Activity_Sobre" />
        <activity android:name=".Activity_Compartilhar" />
        <activity android:name=".Activity_Favoritos" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/TemaSemBarra">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>